{
  "private": true,
  "scripts": {
    "start:dev": "cross-env UMI_ENV=dev TAILWIND_MODE=watch umi dev",
    "start:prod": "cross-env UMI_ENV=prod TAILWIND_MODE=watch umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "autoprefixer": "9",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "less-vars-to-js": "^1.3.0",
    "postcss-px2rem": "^0.3.0",
    "react": "17.x",
    "react-dom": "17.x",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "umi": "^3.5.32"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/fabric": "^3.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.32",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^10.0.7",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
